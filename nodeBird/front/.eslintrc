{
    //기본 eslint가 아닌 babel-eslint가 코드를 해석(최신문법)
    "parser": "babel-eslint",
    "parserOptions":{
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures":{
            "jsx": true
        }
    },
    "env":{
        "brower":true,
        "node":true,
        "es6":true
    },
    "extneds":[
        "airbnb"
        /*"eslint:recommended",
        "plugin:react/recommended" 너무 약함*/
    ],
    "plugins":[
        "import",
        "react-hooks"
    ],
    "rules":{
        "linebreak-style" : 0
    }
}